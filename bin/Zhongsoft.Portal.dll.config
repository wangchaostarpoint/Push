				</binding>
			</customBinding>
			<basicHttpBinding>
				<binding name="baseBind" messageEncoding="Text" textEncoding="Unicode" maxReceivedMessageSize="2147483647" maxBufferPoolSize="2147483647" maxBufferSize="2147483647" transferMode="Buffered">
